        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">6</characteristic>
        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Hex</characteristic>
        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">12+/24+</characteristic>
        <characteristic name="Range" typeId="2954-76ea-decb-30ff">12&quot;/24&quot;</characteristic>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Targets all units within range. Until the start of the casters next turn, the target units suffer -1 to their M, S, I and Acharacteristics (to minimum of 1).</characteristic>
      </characteristics>
    </profile>
    <profile id="d95e-800f-04d2-b736" name="Serpent&apos;s Strength" publicationId="315c-6fd1-56a3-8b04" page="4" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Signature Spell</characteristic>
        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Augment</characteristic>
        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">6+/12+</characteristic>
        <characteristic name="Range" typeId="2954-76ea-decb-30ff">6&quot;/12&quot;</characteristic>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Targets all units within 6&quot; of the Wizard. The unit gains +1 S until the start of the casters next Magic phase.</characteristic>
      </characteristics>
    </profile>
    <profile id="b682-2d03-1694-38da" name="Crystalline Armour" publicationId="315c-6fd1-56a3-8b04" page="5" hidden="false" typeId="7826-5c3d-faa6-1cb7" typeName="Magic Armour">
      <characteristics>
        <characteristic name="Combat" typeId="617f-cb5d-6c83-2017"/>
