        <characteristic name="Range" typeId="2954-76ea-decb-30ff"/>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Enables the wizard to unleash a S 3/4 Breath Weapon attack.</characteristic>
      </characteristics>
    </profile>
    <profile id="f6e1-874b-6f7a-d479" name="Wendala&apos;s Maelstrom" publicationId="315c-6fd1-56a3-8b04" page="55" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">2</characteristic>
