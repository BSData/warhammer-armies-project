    <selectionEntry id="ae5b-c6eb-0e1b-0739" name="The &apos;Marienburg&apos; Class Land Battle Ship" hidden="false" collective="false" import="true" type="model">
      <profiles>
        <profile id="3407-b15d-6eaa-b5ff" name="Land Ship" publicationId="d69c-a7ea-95db-6f56" page="13" hidden="false" typeId="d9ed-a507-0102-d498" typeName="Model">
          <characteristics>
            <characteristic name="M" typeId="09ad-18df-607b-1215"/>
