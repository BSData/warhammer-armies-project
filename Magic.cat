      </characteristics>
    </profile>
    <profile id="8614-b6ab-3e9b-a12b" name="Thunder of Thor" publicationId="5884-1659-bf10-0d5a" page="91" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
