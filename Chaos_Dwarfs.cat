    <profile id="5f9d-cc2e-0318-10a1" name="02 (Dark-forged weapon)" publicationId="faf5-2026-3cea-6e96" page="53" hidden="false" typeId="ed4b-2a78-4af1-73f1" typeName="Darkforged Weapon Chart">
