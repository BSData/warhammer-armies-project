        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">3+</characteristic>
        <characteristic name="Range" typeId="2954-76ea-decb-30ff"/>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">The Patriarch and his unit gain Strength Bonus (1) until the start of the next friendly magic phase.</characteristic>
