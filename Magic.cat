        <characteristic name="Range" typeId="2954-76ea-decb-30ff">12&quot;/18&quot;</characteristic>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">The targeted unit gains Regeneration (5+) until the start of the caster&apos;s next Magic phase.</characteristic>
      </characteristics>
    </profile>
    <profile id="df50-28a8-136c-69cf" name="Shroud of Darkness (Ying)" publicationId="8abe-59b3-1084-5e51" page="108" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">5</characteristic>
        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Hex</characteristic>
