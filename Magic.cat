      </characteristics>
    </profile>
    <profile id="Rip Tide (Bound Spell)" name="Rip Tide (Bound Spell)" publicationId="2053-6f2b-423f-8b0f" page="77" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
