    <profile id="905d-39dd-e3df-a439" name="Virtue of Confidence" publicationId="9e23-79cb-pubN75312" page="179" hidden="false" typeId="5706-c82a-69e3-ce8a" typeName="Virtue of the Chivalric Knight">
