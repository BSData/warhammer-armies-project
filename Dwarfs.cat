          </constraints>
          <profiles>
            <profile id="c500-f637-eedb-593b" name="Valiant Rune" publicationId="aa7b-3583-0834-7bfa" page="208" hidden="false" typeId="6aa2-7707-349b-6651" typeName="Armour Rune">
              <characteristics>
                <characteristic name="Effect" typeId="c6f4-789d-d436-e4d0">The war machine crew gains Stubborn. Multiples of this rune have no further effect.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="5.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="cb8b-596d-5456-612e" name="Rune of Burning" page="" hidden="false" collective="false" import="true" type="upgrade">
