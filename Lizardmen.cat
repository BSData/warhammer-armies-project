                      <characteristics>
                        <characteristic name="Result" typeId="ab47-2fd2-701e-d6d2">2D6 S 5 hits.</characteristic>
                      </characteristics>
                    </profile>
                    <profile id="764a-5810-6253-aa50" name="06" publicationId="9e23-79cb-pubN77402" page="160" hidden="false" typeId="7b2a-3216-1103-5b49" typeName="Beam of Chotec table">
