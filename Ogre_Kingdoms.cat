        <profile id="d0fa-c8c4-6664-c707" name="01" publicationId="9e23-79cb-pubN78137" page="80" hidden="false" typeId="2676-ac77-ba07-72c1" typeName="Ironblaster Misfire Table">
          <characteristics>
            <characteristic name="Result" typeId="eb46-e95b-a434-d5fb">Krakabooom!: The Ironblaster is destroyed and all units within D6&quot; take 2D6 S 5 hits.</characteristic>
          </characteristics>
        </profile>
        <profile id="67a4-f568-f378-958d" name="02" publicationId="9e23-79cb-pubN78137" page="80" hidden="false" typeId="2676-ac77-ba07-72c1" typeName="Ironblaster Misfire Table">
          <characteristics>
            <characteristic name="Result" typeId="eb46-e95b-a434-d5fb">Krrack!: The cannon may not be fired for the rest of the battle.</characteristic>
          </characteristics>
        </profile>
        <profile id="bb58-516c-f73f-132d" name="03-04" publicationId="9e23-79cb-pubN78137" page="80" hidden="false" typeId="2676-ac77-ba07-72c1" typeName="Ironblaster Misfire Table">
          <characteristics>
            <characteristic name="Result" typeId="eb46-e95b-a434-d5fb">Squelch!: The cannon may not fire this turn or next turn.</characteristic>
          </characteristics>
