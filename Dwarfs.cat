            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="3eb4-cf04-6f8b-bb6a" type="max"/>
          </constraints>
          <profiles>
            <profile id="872b-c593-2218-f5cb" name="Master Rune of Defence" publicationId="aa7b-3583-0834-7bfa" page="208" hidden="false" typeId="6aa2-7707-349b-6651" typeName="Armour Rune">
              <characteristics>
                <characteristic name="Effect" typeId="c6f4-789d-d436-e4d0">The War Machine gains a Ward save (3+) against all missile attacks.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="25.0"/>
          </costs>
        </selectionEntry>
