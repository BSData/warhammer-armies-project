    <profile id="8311-e83e-8cb8-10dc" name="Diabolic Splendour" publicationId="1a01-044f-047b-618f" page="243" hidden="false" typeId="49dc-2fd8-b942-dbc2" typeName="Chaos Mutations and Powers">
