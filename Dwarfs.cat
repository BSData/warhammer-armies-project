        <selectionEntry id="311c-b9bd-7660-56d6" name="Rune of Luck" page="" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="9738-b639-e044-749d" type="max"/>
          </constraints>
          <profiles>
            <profile id="834e-4e27-de20-302a" name="Rune of the Luck" publicationId="aa7b-3583-0834-7bfa" page="205" hidden="false" typeId="46dc-ac3a-6194-9dc2" typeName="Talismanic Rune">
              <characteristics>
                <characteristic name="Effect" typeId="2019-7faf-64f7-0c61">One use only. A model with a Rune of Luck can re-roll a single To Hit roll, To Wound roll, armour save, ward save or characteristic test during the game. Multiples of this rune have no further effect.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="5.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="8ded-6191-ff0d-ede0" name="Rune of Brotherhood" page="" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="049f-0c5b-7521-e21d" type="max"/>
