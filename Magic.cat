    <profile id="1d96-d5f1-0119-6daf" name="&apos;Eadbutt (Bound Spell)" publicationId="9e23-79cb-pubN78975" page="199" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
