        <selectionEntry id="48c-fb38-6c4b-f29f" name="Amulet of Hokery-Pokery" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="68bd-8852-51a-cdc4" type="max"/>
