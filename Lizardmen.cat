                      <characteristics>
                        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Innate Bound Spell</characteristic>
                        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Magic Missile</characteristic>
                        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">3+</characteristic>
                        <characteristic name="Range" typeId="2954-76ea-decb-30ff">24&quot;</characteristic>
                        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">If successfully cast, roll a D6 and consult the Beam of Chotec table to determine the S and number of hits (as well as any other effects) caused by this spell. All hits caused by Beam of Chotec are Flaming Attacks.</characteristic>
                      </characteristics>
                    </profile>
                    <profile id="878c-fa95-4a54-70b6" name="01" publicationId="9e23-79cb-pubN77402" page="160" hidden="false" typeId="7b2a-3216-1103-5b49" typeName="Beam of Chotec table">
                      <characteristics>
                        <characteristic name="Result" typeId="ab47-2fd2-701e-d6d2">D3 S 3 hits.</characteristic>
                      </characteristics>
                    </profile>
                    <profile id="4115-5e05-0efd-f958" name="02-03" publicationId="9e23-79cb-pubN77402" page="160" hidden="false" typeId="7b2a-3216-1103-5b49" typeName="Beam of Chotec table">
