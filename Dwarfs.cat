          </constraints>
          <profiles>
            <profile id="cf61-7750-0d19-29aa" name="Stalwart Rune (x2)" publicationId="aa7b-3583-0834-7bfa" page="208" hidden="false" typeId="6aa2-7707-349b-6651" typeName="Armour Rune">
              <characteristics>
                <characteristic name="Effect" typeId="c6f4-789d-d436-e4d0">A war machine engraved with two Stalwart Runes confers a +1 bonus to its crew&apos;s combat result score, and its crew also gain Stubborn. A third Stalwart Rune has no further effect.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="20.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="d55e-979f-4365-32e0" name="Flakkson&apos;s Rune of Seeking" page="" hidden="false" collective="false" import="true" type="upgrade">
