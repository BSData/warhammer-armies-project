            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="71c2-f706-1f22-72c9" type="max"/>
          </constraints>
          <profiles>
            <profile id="9631-302f-8f59-e3d1" name="Rune of Forging" publicationId="aa7b-3583-0834-7bfa" page="208" hidden="false" typeId="6aa2-7707-349b-6651" typeName="Armour Rune">
              <characteristics>
                <characteristic name="Effect" typeId="c6f4-789d-d436-e4d0">Cannon only. A war machine engraved with a Rune of Forging can re-roll the artillery dice whenever a misfire result is rolled; however, you must accept the result of the second roll. Multiples of this rune have no further effect.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="25.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="a132-2d64-82c3-8d39" name="Stalwart Rune (x1)" page="" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="cf96-38ec-0131-dee8" type="max"/>
          </constraints>
          <profiles>
            <profile id="ba70-0b0e-7741-a2d9" name="Stalwart Rune (x1)" publicationId="aa7b-3583-0834-7bfa" page="208" hidden="false" typeId="6aa2-7707-349b-6651" typeName="Armour Rune">
              <characteristics>
                <characteristic name="Effect" typeId="c6f4-789d-d436-e4d0">A war machine engraved with a Stalwart Rune confers a +1 bonus to its crew&apos;s combat result score.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="10.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="d19d-db75-2717-ffe4" name="Stalwart Rune (x2)" page="" hidden="false" collective="false" import="true" type="upgrade">
