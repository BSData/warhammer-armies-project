    <profile id="267a-c6dd-0d64-ed05" name="Ursun&apos;s Roar" publicationId="1607-bfe9-82af-d3dc" page="14" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Innate Bound Spell</characteristic>
