    <profile id="6b4d-ca97-1223-ca26" name="Rune of Stone and Steel" publicationId="aa7b-3583-0834-7bfa" page="141" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Innate bound spell</characteristic>
        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Augment</characteristic>
