        <characteristic name="Range" typeId="2954-76ea-decb-30ff">18&quot;/36&quot;</characteristic>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Causes 4D6 S 2 hits.</characteristic>
      </characteristics>
    </profile>
    <profile id="0677-3082-d83e-e92a" name="Embrace of the Serpent" publicationId="315c-6fd1-56a3-8b04" page="55" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
