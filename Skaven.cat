        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">5+</characteristic>
        <characteristic name="Range" typeId="2954-76ea-decb-30ff">12&quot;</characteristic>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">The target model (which must be an Infantry character) may immediately be placed anywhere on the battlefield, but at least 1&quot; away from enemy models.</characteristic>
      </characteristics>
    </profile>
    <profile id="9701-0863-ac83-591c" name="Armour of Darkness" publicationId="4086-1ccd-f433-39a2" page="113" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a"/>
