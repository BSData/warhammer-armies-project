    <profile id="d23c-1b94-db35-de07" name="Absorbing Chill (Ying)" publicationId="8abe-59b3-1084-5e51" page="108" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">2</characteristic>
