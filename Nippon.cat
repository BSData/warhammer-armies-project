        <characteristic name="Missile" typeId="97ea-ecf7-8d24-b2ca">6+</characteristic>
        <characteristic name="Special Rules" typeId="ff58-91e4-84d7-e6e4"/>
      </characteristics>
    </profile>
    <profile id="9cc5-6672-fc28-bed4" name="War Fan" publicationId="5ff9-69dd-8810-183f" page="77" hidden="false" typeId="a83b-63ee-855d-d557" typeName="Equipment">
      <characteristics>
        <characteristic name="Effect" typeId="6b91-16f4-1f8b-f289"/>
      </characteristics>
    </profile>
    <profile id="1b22-1608-c977-7fbf" name="Kirin" publicationId="5ff9-69dd-8810-183f" page="70" hidden="false" typeId="d9ed-a507-0102-d498" typeName="Model">
