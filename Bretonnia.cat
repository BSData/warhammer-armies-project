                <selectionEntry id="2223-223b-a1e7-2905" name="The Chalice of Potions" hidden="false" collective="false" import="true" type="upgrade">
                  <constraints>
                    <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="0527-5a72-9260-932a" type="min"/>
