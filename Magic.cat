    <profile id="a012-5764-3f62-c2bb" name="Pavane of Slaanesh (Bound Spell)" publicationId="eee5-f9c5-fa08-3f2e" page="112, 201, 240" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">2</characteristic>
        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Direct Damage</characteristic>
