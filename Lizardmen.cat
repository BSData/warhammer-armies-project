              </constraints>
              <rules>
                <rule id="a01d-9e17-e421-98ac" name="Solar Engine" publicationId="9e23-79cb-pubN77402" page="160" hidden="false">
                  <description>All friendly Cold-blooded units within 6&quot; of one or more Bastiladons with a Solar Engine gain +1 I. In addition, a Solar Engine contains the innate bound spell, Beam of Chotec.</description>
                </rule>
              </rules>
              <selectionEntries>
                <selectionEntry id="5e79-27ce-7db4-2c1d" name="Beam of Chotec" hidden="false" collective="false" import="true" type="upgrade">
                  <constraints>
                    <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="true" includeChildForces="false" id="576d-df0c-52ab-d1ed" type="max"/>
