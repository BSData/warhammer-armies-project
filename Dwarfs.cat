          </constraints>
          <profiles>
            <profile id="e070-7297-963f-1934" name="Flakkson&apos;s Rune of Seeking" publicationId="aa7b-3583-0834-7bfa" page="208" hidden="false" typeId="6aa2-7707-349b-6651" typeName="Armour Rune">
              <characteristics>
                <characteristic name="Effect" typeId="c6f4-789d-d436-e4d0">Bolt Throwers only. A war machine engraved with Flakkson&apos;s Rune of Seeking gains +1 To Hit against any Flying units. Multiples of this rune have no further effect.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="15.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="dcf1-acd8-e18e-6e06" name="Rune of Fortune" page="" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="0418-c8ab-c32f-6a8a" type="max"/>
          </constraints>
          <profiles>
            <profile id="0e70-8f0b-f75a-6629" name="Rune of Fortune" publicationId="aa7b-3583-0834-7bfa" page="208" hidden="false" typeId="6aa2-7707-349b-6651" typeName="Armour Rune">
              <characteristics>
                <characteristic name="Effect" typeId="c6f4-789d-d436-e4d0">If a machine has the Rune of Fortune the player may re-roll the result on the machine&apos;s Misfire chart. Multiples of this rune have no further effect.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="10.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="bcb2-04bc-8c1f-6303" name="Valiant Rune" page="" hidden="false" collective="false" import="true" type="upgrade">
