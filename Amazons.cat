        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">5+/8+</characteristic>
        <characteristic name="Range" typeId="2954-76ea-decb-30ff"/>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">The Wizard makes a Breath Weapon Attack. This may be cast in close combat, following the normal rules for Breath Weapons. All models Hit suffer a Strength 4/5 Hit.</characteristic>
      </characteristics>
    </profile>
    <profile id="b8b9-bb23-9993-1987" name="Spirit Walk" publicationId="315c-6fd1-56a3-8b04" page="4" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Lore Attribute</characteristic>
        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b"/>
        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0"/>
        <characteristic name="Range" typeId="2954-76ea-decb-30ff"/>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Whenever a spell from the Lore of the Serpent is successfully cast on a friendly unit, it gains +D6 to their M value and does not need to take any tests for Dangerous Terrain until the start of the caster&apos;s next magic phase.</characteristic>
      </characteristics>
    </profile>
    <profile id="d9d8-c1fe-6405-4231" name="Siren&apos;s Dream" publicationId="315c-6fd1-56a3-8b04" page="4" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
