    <profile id="3cdb-e1c2-77c0-cb7f" name="Flight of Sylra" publicationId="5884-1659-bf10-0d5a" page="91" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">2</characteristic>
        <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Augment</characteristic>
