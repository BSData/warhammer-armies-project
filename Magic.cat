      </characteristics>
    </profile>
    <profile id="ef8b-c606-c570-aa0f" name="Plague Rash" publicationId="4086-1ccd-f433-39a2" page="226" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
