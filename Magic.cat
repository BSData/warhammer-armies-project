    </profile>
    <profile id="eabc-91ad-837b-4188" name="Bone Krusha" publicationId="9e23-79cb-pubN78975" page="200" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Signature Spell</characteristic>
