    <profile id="5c0b-dd81-de52-0983" name="Virtue of the Joust" publicationId="9e23-79cb-pubN75312" page="179" hidden="false" typeId="5706-c82a-69e3-ce8a" typeName="Virtue of the Chivalric Knight">
      <characteristics>
