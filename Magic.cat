        <characteristic name="Range" typeId="2954-76ea-decb-30ff">24&quot;</characteristic>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Causes D6 S 6 hits with Lightning Attacks.</characteristic>
      </characteristics>
