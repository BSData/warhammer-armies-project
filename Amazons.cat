      </characteristics>
    </profile>
    <profile id="b0b3-52ba-3552-52ce" name="Singing Wind" publicationId="315c-6fd1-56a3-8b04" page="4" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
