        <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">7+/10+</characteristic>
        <characteristic name="Range" typeId="2954-76ea-decb-30ff">18&quot;/36&quot;</characteristic>
        <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Inflicts D6 S 5 hits for every friendly unit of 5 or more Orc models (of any race) within 6&quot; of the caster.</characteristic>
      </characteristics>
    </profile>
    <profile id="858b-3806-6cc4-9a3c" name="WAAAGH!" publicationId="9e23-79cb-pubN78975" page="199" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
      <characteristics>
        <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">5</characteristic>
